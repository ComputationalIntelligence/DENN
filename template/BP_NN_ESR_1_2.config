//info
var workers 0, seed date("%S%H%d%m%Y")
//dataset
var samples 3910, batch 5,
    input "datasets/esr_1_2_novalidation_normalized.db.gz"
//backpropagation
var epoch 10
//output
var output "BP_NN_ESR_1_2.json", full_output "results/" + $output, stream "::cout"
////////////////////////////////////////////////////////
//network
network
{
    fc[50]
    fc[/*auto*/]
    softmax
}

//epoch
generations $samples / $batch * $epoch 
sub_gens max(floor(( $samples / $batch ) * 0.1), 1)

//init individuals
distribution normal {
    normal_mu 0
    normal_sigma 0.01
}

//backpropagation
instance backpropagation
learning_rate 0.001
decay 0.00005
momentum 0.9
nesterov true

//Others
seed $seed
output $full_output
runtime_output_file $stream
dataset $input
batch_size $batch
batch_offset $batch
